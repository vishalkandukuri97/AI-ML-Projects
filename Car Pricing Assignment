        "        plt.title(f\"{col} vs Price\")\n",
        "        plt.xlabel(col)\n",
        "        plt.ylabel(\"Price\")\n",
        "\n",
        "        # Add a trend line\n",
        "        x = df[col].values\n",
        "        y = df[\"price\"].values\n",
        "        mask = ~np.isnan(x) & ~np.isnan(y)\n",
        "        if mask.sum() > 2:\n",
        "            coeffs = np.polyfit(x[mask], y[mask], 1)\n",
        "            x_line = np.linspace(x[mask].min(), x[mask].max(), 100)\n",
        "            y_line = coeffs[0]*x_line + coeffs[1]\n",
        "            plt.plot(x_line, y_line, color='red')\n",
        "        plt.show()"
      ],
      "metadata": {
        "id": "M8hgEZJj9X-d"
